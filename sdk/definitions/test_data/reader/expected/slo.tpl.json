[
  {
    "apiVersion": "n9/v1alpha",
    "kind": "SLO",
    "metadata": {
      "name": "get-store-p95-latency-rolling",
      "project": "lightstep-raw"
    },
    "spec": {
      "description": "",
      "service": "android-service",
      "indicator": {
        "metricSource": {
          "name": "lightstep"
        }
      },
      "timeWindows": [
        {
          "unit": "Day",
          "count": 7,
          "isRolling": true
        }
      ],
      "budgetingMethod": "Occurrences",
      "objectives": [
        {
          "displayName": "Good",
          "name": "objective-1",
          "op": "lte",
          "rawMetric": {
            "query": {
              "lightstep": {
                "streamId": "DzpxcSRh",
                "typeOfData": "latency",
                "percentile": 95
              }
            }
          },
          "value": 150,
          "target": 0.50
        }
      ],
      "alertPolicies": null
    },
    "manifestSrc": "{{ .ManifestSrc }}"
  }
]
